#include <bits/stdc++.h>
using namespace std;
 
int main() {
    int A, B;
    char op;
    cin >> A >> op >> B;
 
    switch (op) {
        case '+':
            cout << A + B << endl;
            break;
        case '-':
            cout << A - B << endl;
            break;
        case '*':
            cout << A * B << endl;
            break;
        case '/':
            cout << A / B << endl; 
    }
 
    return 0;
}
